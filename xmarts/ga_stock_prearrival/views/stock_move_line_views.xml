<odoo>
	 <data>
	        
		<record id="stock_move_line_operations_tree_prearrival" model="ir.ui.view">
			<field name="name">stock.move.line.operations.tree.prearrival</field>
			<field name="model">stock.move.line</field>
			<field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='qty_done']" position="after">
					<button name="action_assign_destination_location" string="Assign Destination Location" type="object" icon="fa-plus-square"/>
				</xpath>
			</field>
		</record>             
	             
	</data>
</odoo>